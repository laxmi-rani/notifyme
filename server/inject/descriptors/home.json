{
  "type": "object",
  "comment": "Home",
  "properties": {
    "search": {
      "type": "string",
      "selector": "#lst-ib",
      "control": "text"
    },
    "searchbutton": {
      "type": "action",
      "selector": "[name=\"btnK\"]",
      "control": "action.button"
    },
    "isGoogle": {
      "type": "identifier",
      "selector": "body",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isGoogle"
      ],
      "event": "triggerworkday"
    },
    {
      "match": [
        "isGoogle"
      ],
      "screens": [
        "home"
      ]
    }
  ]
}
